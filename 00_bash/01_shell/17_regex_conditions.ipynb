   "source": [
    "# Regexp Conditions\n",
    "\n",
    "| Test    | Meaning |\n",
    "|---------|---------|\n",
    "|`[[ STR =~ REGEX ]]`| Regexp |"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Examples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0\n",
      "1\n"
     ]
    }
   ],
   "source": [
    "%%bash\n",
    "pattern='<a href=\"(.+)\">link</a>'\n",
    "[[ '<a href=\"foo.com\">link</a>' =~ $pattern ]]\n",
    "echo $? # true\n",
    "\n",
    "[[ '<a href=\"bar.com\">another link</a>' =~ $pattern ]]\n",
    "echo $? # false"
   ]
  },
