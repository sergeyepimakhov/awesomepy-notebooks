  },
  {
   "cell_type": "code",
   "execution_count": 333,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      ".\n",
      "├── dir0\n",
      "├── dir1\n",
      "├── dir2\n",
      "├── dir3\n",
      "├── file0\n",
      "├── file1\n",
      "├── file2\n",
      "├── file3\n",
      "├── file_renamed\n",
      "├── my_dir\n",
      "├── my_pics\n",
      "└── my_videos\n",
      "\n",
      "7 directories, 5 files\n"
     ]
    }
   ],
   "source": [
    "%%ashhhh\n",
